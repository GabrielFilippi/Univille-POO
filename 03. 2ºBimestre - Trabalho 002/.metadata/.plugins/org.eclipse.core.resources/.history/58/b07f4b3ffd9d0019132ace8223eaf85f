package com.gabriel.app.radiofm;

import com.gabriel.app.calendario.Calendario;
import com.gabriel.app.calendario.CalendarioFormatter;
import com.gabriel.app.display.Display;

public class RadioFmDisplay extends Display<RadioFm> {

	private RadioFmFormatter cFormatter = new RadioFmFormatter();

	private Calendario calendario;
	
	//construtor
	public CalendarioDisplay() {
		this.calendario = new Calendario();
		this.calendario.setDia(5);
		this.calendario.setMes(7);
		this.calendario.setAno(2019);
	}
	
	public void show() {
		System.out.println(cFormatter.formatter(calendario));
	}
	
	
	
	
	
	
	
	private double estacao;

	public void show() {

	}

}
